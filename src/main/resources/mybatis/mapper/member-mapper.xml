<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.comboard.dao.IMemberDao">
	<select id="selectAll" resultType="com.example.comboard.dto.MemberDto">
		SELECT * FROM member
	</select>
	<select id="selectOne" resultType="com.example.comboard.dto.MemberDto">
		SELECT * FROM member WHERE mno = #{mno}
	</select>
</mapper>